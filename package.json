{
  "name": "nuxt-testable",
  "description": "Nuxt.js TypeScript project",
  "version": "2.11.0",
  "private": true,
  "dependencies": {
    "@nuxt/typescript-runtime": "0.3.6",
    "@nuxtjs/axios": "5.9.0",
    "nuxt": "3.4.3",
    "nuxt-property-decorator": "2.5.0",
    "provide-consume-decorator": "^1.0.0",
    "vuex-class": "0.3.2",
    "vuex-module-decorators": "^0.11.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "test": "NODE_ENV=test jest"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@nuxt/typescript-build": "0.5.5",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "vue-jest": "^3.0.5"
  },
  "jest": {
    "testRegex": "(/__tests__/*|(\\.|/)spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testURL": "http://localhost/"
  },
  "babel": {
    "plugins": [
      [
        "dynamic-import-node",
        {
          "noInterop": true
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  }
}
